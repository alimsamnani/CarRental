
@{
    ViewBag.Title = "Cars";
    Layout = "~/Views/Shared/_Master.cshtml";
}
<!--== Page Title Area Start ==-->
<section id="page-title-area" class="section-padding overlay">
    <div class="container">
        <div class="row">
            <!-- Page Title Start -->
            <div class="col-lg-12">
                <div class="section-title  text-center">
                    <h2>Car List</h2>
                    <span class="title-line"><i class="fa fa-car"></i></span>
                    <p>List Of Car along with details.</p>
                </div>
            </div>
            <!-- Page Title End -->
        </div>
    </div>
</section>

<div class="contact-page-wrao section-padding">
    <div class="container">
        <div class="row">
            <div class="col-lg-12" style="float:right">
                <a href="/AddCar/Add" class="btn btn-primary" style="float:right">Add New Car</a>
            </div>
            
            <table class="table">
                <thead>
                    <tr>
                        <th>Car Name</th>
                        <th>Category</th>
                        <th>Car Make</th>
                        <th>Car Model</th>
                        <th>Num Of Doors</th>
                        <th>Num Of Seats</th>
                        <th>Car has AC?</th>
                        <th>Type Of Car</th>
                        <th>Rental Charges</th>
                        <th>Mileage Allowed</th>
                        <th>Buffer Days</th>
                        <th>Images</th>
                        <th>Edit</th>
                        <th>Delete</th>
                    </tr>
                </thead>
                <tbody>
                    @Html.Raw(ViewBag.htmlString)
                </tbody>
            </table>

        </div>

    </div>
</div>